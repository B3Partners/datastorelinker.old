<?xml version="1.0" encoding="UTF-8"?>
<Context path="/datastorelinker">
    <Parameter name="publisher.serverUrl" override="false" value="http://localhost:8080/geoserver"/>
    <Parameter name="publisher.serviceUser" override="false" value="admin"/>
    <Parameter name="publisher.servicePassword" override="false" value="geoserver"/>
    <Parameter name="publisher.defaultSRS" override="false" value="point"/>
    <Parameter name="publisher.defaultLineStyle" override="false" value="line"/>
    <Parameter name="publisher.defaultPolygonStyle" override="false" value="polygon"/>
    <Parameter name="publisher.mapfileLocation" override="false" value="MAPFILE_LOCATION"/>
    <!-- de (geoserver) workspace waar de services gemaakt worden -->
    <Parameter name="publisher.serviceName" override="false" value="lokaal"/>
    <!-- optionele SRS voor te publiceren services, default is EPSG:28992 of 90112/28992 -->
    <Parameter name="publisher.serviceSRS" override="false" value="EPSG:28992"/>
</Context>
